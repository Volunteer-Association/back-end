<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-starter-parent</artifactId>
        <version>3.1.0</version>
        <relativePath/> <!-- lookup parent from repository -->
    </parent>

    <groupId>com.example</groupId>
    <artifactId>back-end-backend</artifactId>
    <version>${revision}</version>
    <packaging>pom</packaging>
    <name>${project.artifactId}</name>
    <description>back-end-backend</description>

    <modules>
        <module>back-end-api</module>
        <module>back-end-service</module>
    </modules>

    <properties>
        <java.version>17</java.version>
        <!--项目版本号-->
        <revision>1.0.0-SNAPSHOT</revision>

        <!--依赖的rbac版本-->
        <zz.rbac.version>1.0.0-SNAPSHOT</zz.rbac.version>

        <!--依赖的base版本-->
        <zz.base.version>1.0.0-SNAPSHOT</zz.base.version>
    </properties>

</project>
